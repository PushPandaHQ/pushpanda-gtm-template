___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// PushPanda.io Tag Code
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');

// If the user chose to log debug output, initialize the logging method
const log = data.pp_debug ? logToConsole : (() => {});

// get pp array
const pp = copyFromWindow('_pushpanda') || [];

// add variables
pp.push(['_project', data.pp_id]);
pp.push(['_path', data.pp_path]);


if (data.pp_debug) {
	pp.push(['_debug', true]);
}

// emit pp config array
setInWindow('_pushpanda', pp, true);

// load pp sdk
const url = '//cdn.pushpanda.io/sdk/sdk.js';
injectScript(url, data.gtmOnSuccess(), data.gtmOnFailure(), url);


